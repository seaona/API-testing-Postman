						"exec": [
							"const response = pm.response.json();",
							"console.log(response.uuid);",
							"",
							"pm.globals.set(\"orderId\", response.uuid);"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
